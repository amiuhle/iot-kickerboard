<table class='players'>
  <thead>
    <tr>
      <th class='right'>Id</th>
      <th class='left'>Shooter</th>
      <th class='left'>Target</th>
    </tr>
  </thead>
  <tbody>
    {{#each shots}}
      <tr>
        <td>{{id}}</td>
        <td>{{shooter}}</td>
        <td>{{target}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
